pipeline{
    agent any
      environment {
    notifyEmail ="alpana@nagarro.com"
  }
    stages{
        stage("Code Checkout"){
            steps{
            bat "echo checkout"
            }
        }
        stage("Code test"){
            steps{
            bat "echo test"
            }
        }
        stage("Code deployed"){
            steps{
            bat "echo deployed"
            }
        }
    }
    post{
        success{
            bat "echo success"
            }
        
        }
}
